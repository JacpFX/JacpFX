<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="291.0"   HBox.hgrow="ALWAYS" xmlns:fx="http://javafx.com/fxml" >
  <children>
    <VBox fx:id="chatField" alignment="CENTER" maxWidth="-1.0" styleClass="roundedAnchorPane" VBox.vgrow="ALWAYS">
      <padding>
        <Insets bottom="10.0" left="15.0" right="15.0" top="10.0" />
      </padding>
      <VBox.margin>
        <Insets left="10.0" right="10.0" top="10.0" />
      </VBox.margin>
    </VBox>
    <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefHeight="-1.0" VBox.vgrow="NEVER">
      <children>
        <TextField id="synopsis" fx:id="message" maxWidth="-1.0" prefHeight="27.0" styleClass="descriptionValue" text="" HBox.hgrow="ALWAYS">
          <HBox.margin>
            <Insets right="30.0" />
          </HBox.margin>
        </TextField>
        <Button id="button1" fx:id="sendButton" alignment="BOTTOM_RIGHT" onAction="#handleSend" text="%send" />
      </children>
      <padding>
        <Insets bottom="10.0" left="15.0" right="15.0" top="10.0" />
      </padding>
    </HBox>
  </children>
</VBox>
